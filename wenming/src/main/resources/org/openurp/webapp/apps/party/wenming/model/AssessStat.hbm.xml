<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
    <class name="AssessStat">
        <id name="id" type="long">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <many-to-one name="department" column="department_id" not-null="true"/>
        <many-to-one name="session" column="session_id" not-null="true"/>
        <many-to-one name="schema" column="schema_id" not-null="true"/>
        <property name="selfScore" not-null="true"/>
        <property name="mutualScore" not-null="true"/>
        <property name="supervisorScore" not-null="true"/>
        <property name="score" not-null="true"/>
        <property name="statAt" not-null="true"/>
        
        <bag name="items" inverse="true" cascade="all">
          <key column="stat_id"></key>
          <one-to-many class="org.openurp.webapp.apps.party.wenming.model.AssessStatItem" />
        </bag>
    </class>
</hibernate-mapping>
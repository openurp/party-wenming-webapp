<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
	<class name="AssessSchema">
		<id name="id" type="integer">
			<generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
		</id>
		<property name="name" length="100" not-null="true" />
		<bag name="groups" inverse="true" cascade="all">
			<key column="schema_id" not-null="true" />
			<many-to-many column="group_id"
				class="org.openurp.webapp.apps.party.wenming.model.AssessItemGroup" />
		</bag>
		<bag name="departs" inverse="true" cascade="all">
			<key column="schema_id" not-null="true" />
			<many-to-many column="department_id"
				class="org.openurp.kernel.base.unit.model.Department" />
		</bag>
	</class>
</hibernate-mapping>
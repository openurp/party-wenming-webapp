<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.depart.model">
    <class name="AssessStatItem">
        <id name="id" type="long">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <many-to-one name="stat" column="stat_id" not-null="true"/>
        <property name="selfScore" not-null="true"/>
        <property name="mutualScore" not-null="true"/>
        <property name="supervisorScore" not-null="true"/>
        <property name="score" not-null="true"/>
    </class>
</hibernate-mapping>
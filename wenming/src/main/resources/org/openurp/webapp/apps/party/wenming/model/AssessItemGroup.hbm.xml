<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
    <class name="AssessItemGroup">
        <id name="id" type="integer">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <property name="name" length="100" not-null="true" />
        <property name="indexno" length="100" not-null="true" />
        <many-to-one name="parent" column="parent_id" class="org.openurp.webapp.apps.party.wenming.model.AssessItemGroup"></many-to-one>
        <many-to-one name="schema" column="schema_id" class="org.openurp.webapp.apps.party.wenming.model.AssessSchema"></many-to-one>
        <bag name="children" inverse="true" cascade="all">
          <key column="parent_id"></key>
          <one-to-many class="org.openurp.webapp.apps.party.wenming.model.AssessItemGroup" />
        </bag>
        <bag name="items" inverse="true" cascade="all">
          <key column="group_id"></key>
          <one-to-many class="org.openurp.webapp.apps.party.wenming.model.AssessItem" />
        </bag>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
  <class name="AssessSessionBean">
    <id name="id">
      <generator class="org.beangle.orm.hibernate.TableSeqGenerator"/>
    </id>
    <property name="name" length="100" not-null="true"/>
    <property name="enabled" not-null="true"/>
    <property name="opened" not-null="true"/>
    <property name="beginOn" not-null="true"/>
    <property name="endOn" not-null="true"/> 
    <property name="voteBeginOn" not-null="true"/>
    <property name="voteEndOn" not-null="true"/>
	<property name="updatedAt"/>
  </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
 <class name="WenmingVoteSession">
  <id name="id" type="long">
   <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
  </id>
  <property name="name" not-null="true"/>
  <property name="beginOn" not-null="true"/>
  <property name="endOn" not-null="true"/>
  <property name="limit" column="limit_" not-null="true"/>
  <property name="limit2" column="limit2_" not-null="true"/>
  <property name="limit3" column="limit3_" not-null="true"/>
  <many-to-one name="session" column="session_id" not-null="true" />
  <bag name="voters">
   <key column="session_id"></key>
   <many-to-many column="voter_id"
    class="org.openurp.webapp.apps.party.wenming.model.WenmingProjectVoter" />
  </bag>
  <bag name="projects">
   <key column="session_id"></key>
   <many-to-many column="project_id" class="org.openurp.webapp.apps.party.wenming.project.model.GoodProject" />
  </bag>
  <bag name="persons">
   <key column="session_id"></key>
   <many-to-many column="person_id" class="org.openurp.webapp.apps.party.wenming.person.model.GoodPerson" />
  </bag>
  <bag name="posts">
   <key column="session_id"></key>
   <many-to-many column="post_id" class="org.openurp.webapp.apps.party.wenming.post.model.GoodPost" />
  </bag>
 </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
    <class name="WenmingProjectVoter">
        <id name="id" type="integer">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <property name="name" length="100" not-null="true" />
        <property name="fullname" length="100" not-null="true" />
        <property name="password" length="100" />
        <property name="remark" length="100" />
        <property name="expiredOn"/>
    </class>
</hibernate-mapping>
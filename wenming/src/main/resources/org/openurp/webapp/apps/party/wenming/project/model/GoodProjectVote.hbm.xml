<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.project.model">
    <class name="GoodProjectVote">
        <id name="id" type="long">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <many-to-one name="goodProject" column="good_project_id" not-null="true"/>
        <many-to-one name="session" column="session_id" not-null="true"/>
        <many-to-one name="voter" column="voter_id" not-null="true"/>
        <property name="createdAt"  not-null="true"/>
        <property name="updatedAt"  not-null="true"/>
        <property name="ayes"  not-null="true"/>
        <property name="submit" not-null="true"/>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
    <class name="SupervisorAssess">
        <id name="id" type="long">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <many-to-one name="assessDepartment" column="assess_department_id"  not-null="true"/>
        <many-to-one name="department" column="department_id" not-null="true"/>
        <many-to-one name="session" column="session_id" not-null="true"/>
        <many-to-one name="schema" column="schema_id" not-null="true"/>
        <property name="assessAt" not-null="true"/>
        <property name="totalScore" not-null="true"/>
        
        <many-to-one name="assessBy" column="assess_by_id" not-null="true"/>
        <property name="state" not-null="true" type="assessState"/>
        <bag name="items" inverse="true" cascade="all">
          <key column="assess_id"></key>
          <one-to-many class="org.openurp.webapp.apps.party.wenming.model.SupervisorAssessItem" />
        </bag>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.webapp.apps.party.wenming.model">
    <class name="AbstractWenmingSummaryObject" abstract="true">
        <id name="id" type="integer">
            <generator class="org.beangle.orm.hibernate.TableSeqGenerator" />
        </id>
        <property name="state"  type="assessState" />
        <component name="attachment" >
            <property name="name" length="200" column="attach_name"/>
            <property name="filePath"  length="100" column="attach_file_path"/>
        </component>
        <property name="createdAt"  not-null="true"/> 
        <many-to-one  name="user" column="user_id" not-null="true"/> 
        <union-subclass name="org.openurp.webapp.apps.party.wenming.model.AbstractWenmingFinalSummaryObject" abstract="true">
	        <property name="content" length="1000" not-null="true"/>
	        <property name="forAssess" not-null="true"/>
	      <union-subclass name="org.openurp.webapp.apps.party.wenming.project.model.GoodProjectFinalSummary">
          <property name="auditOpinion"  />  
	        <many-to-one  name="good" column="good_id" not-null="true"/> 
	      </union-subclass>  
        </union-subclass>
        <union-subclass name="org.openurp.webapp.apps.party.wenming.project.model.GoodProjectMiddleSummary">  
        <many-to-one  name="good" column="good_id" not-null="true"/> 
        </union-subclass>
    </class>
</hibernate-mapping>
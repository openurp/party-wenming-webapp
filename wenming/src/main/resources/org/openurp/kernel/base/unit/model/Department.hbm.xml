<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.kernel.base.unit.model">
  <class name="Department">
    <id name="id" type="integer">
      <generator class="org.beangle.orm.hibernate.TableSeqGenerator"/>
    </id>
    <property name="code" length="100" not-null="true"/>
    <property name="name" length="100" not-null="true"/>
    <property name="beginOn" />
    <property name="endOn"/>
    <property name="teaching"/>
  </class>
</hibernate-mapping>